{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug compile",
            "cargo": {
                "args": [
                    "build",
                    "--features=debug",
                    "--bin=${fileBasenameNoExtension}",
                ],
            },
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'brack'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=brack",
                    "--package=brack"
                ],
                "filter": {
                    "name": "brack",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
